<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape android:shape="oval">
            <stroke android:width="@dimen/_8dp" android:color="@color/md_white"/>
            <solid android:color="@color/md_blue_grey_500" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="oval">
            <stroke android:width="@dimen/_8dp" android:color="@color/md_blue_grey_500" />
            <solid android:color="@color/md_white"/>
        </shape>
    </item>
</selector>
