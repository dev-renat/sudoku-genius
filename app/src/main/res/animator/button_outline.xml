<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <set>
            <objectAnimator
                android:propertyName="backgroundColor"
                android:valueFrom="?attr/buttonOutlineBGColorStatePressedTrue"
                android:valueTo="?attr/buttonOutlineBGColorStatePressedFalse"
                android:duration="200"
                android:valueType="intType" />
        </set>
    </item>
    <item android:state_pressed="true">
        <set>
            <objectAnimator
                android:propertyName="backgroundColor"
                android:valueFrom="?attr/buttonOutlineBGColorStatePressedFalse"
                android:valueTo="?attr/buttonOutlineBGColorStatePressedTrue"
                android:duration="200"
                android:valueType="intType" />
        </set>
    </item>
</selector>
